# ===== Provider keys (required for hybrid/cloud mode) =====
OPENROUTER_API_KEY=
NVIDIA_API_KEY=
OPENAI_API_KEY=

# ===== LiteLLM =====
LITELLM_MASTER_KEY=change-me
LITELLM_BASE_URL=http://litellm:4000

# ===== Skyvern =====
SKYVERN_BASE_URL=http://skyvern:8000
SKYVERN_API_KEY=
SKYVERN_TASK_ENDPOINT=/api/v1/tasks
SKYVERN_DB_USER=skyvern
SKYVERN_DB_PASSWORD=skyvern
SKYVERN_DB_NAME=skyvern

# ===== Runtime storage =====
MEMORY_DB_PATH=/data/memory/skyagentos.db
AGENT_FS_ROOT=agentfs
DESKTOP_DAEMON_URL=http://desktop-daemon:8890

# ===== Runtime controls =====
MAX_SELF_CORRECTIONS=3
SKYAGENT_BUDGET_USD=2.0
SKYAGENT_TEMPLATE=web_research
SKYAGENT_DOMAIN=general
SKYAGENT_DRY_RUN=true

# ===== OpenClaw channels =====
OPENCLAW_WHATSAPP_ENABLED=true
OPENCLAW_TELEGRAM_ENABLED=true

# ===== Grafana (required; no insecure default) =====
GF_SECURITY_ADMIN_USER=
GF_SECURITY_ADMIN_PASSWORD=
